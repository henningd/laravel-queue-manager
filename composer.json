{
    "name": "henningd/laravel-queue-manager",
    "description": "A comprehensive Laravel package for managing queues and workers with a beautiful dashboard",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Henning D",
            "email": "henning@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "HenningD\\LaravelQueueManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HenningD\\LaravelQueueManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HenningD\\LaravelQueueManager\\QueueManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "queue",
        "worker",
        "management",
        "dashboard",
        "monitoring"
    ]
}